
#
# Location of the built externals-clasp
export EXTERNALS_CLASP_HASH = `ros config show externals.clasp.version`
export EXTERNALS_CLASP_DIR = ${HOME}/.roswell/lib/x86-64/linux/externals-clasp/${EXTERNALS_CLASP_HASH}

# Location of clasp
export CLASP_HOME = ${HOME}/.roswell/src/clasp

# The Clasp runtime you want to use (boehm, mps, boehmdc)
export CLASP_RUNTIME = boehm

export CLANG = ${EXTERNALS_CLASP_DIR}/llvm39ToT/build-release/bin/clang++
